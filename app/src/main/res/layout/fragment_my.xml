<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_my_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@mipmap/yh_bj">

        <RelativeLayout
            android:id="@+id/rl_my_header"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_46"
            android:layout_marginTop="20dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="我的"
                android:textColor="@color/white"
                android:textSize="@dimen/sp_18" />

        </RelativeLayout>
         <RelativeLayout
             android:id="@+id/rl_my_to"
             android:layout_below="@+id/rl_my_header"
             android:layout_width="match_parent"
             android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/iv_my_pic"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_10"
            android:src="@mipmap/yh_tx" />

        <TextView
            android:id="@+id/tv_my_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/iv_my_pic"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_15"
            android:text="经销商"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_14"

            />

        <TextView
            android:id="@+id/tv_my_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_my_name"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_3"
            android:text="13333333333"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_12"

            />
         </RelativeLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/rl_my_main"
        android:layout_below="@+id/rl_my_top"
        android:background="@drawable/bg_yhbj"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginTop="-45dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
       <RelativeLayout
           android:id="@+id/rl_my_name"
           android:layout_width="match_parent"
           android:layout_height="@dimen/dp_45">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="配置设备"
            android:layout_toRightOf="@+id/iv_my_pz"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_15"
            android:textColor="#6d6d6d"
            android:textSize="@dimen/sp_15"
            />
           <ImageView
               android:id="@+id/iv_my_pz"
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_centerVertical="true"
               android:layout_marginLeft="@dimen/dp_15"
               android:src="@mipmap/my_pzsb"
               />
           <ImageView

               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:layout_centerVertical="true"
               android:layout_alignParentRight="true"
               android:layout_marginRight="@dimen/dp_15"
               android:src="@mipmap/menu_xzn"
               />
           <View
               android:layout_width="wrap_content"
               android:layout_height="1dp"
               android:layout_alignParentBottom="true"
               android:layout_marginLeft="@dimen/dp_20"
               android:layout_marginRight="@dimen/dp_20"
               android:background="#eeeeee"
               />
       </RelativeLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_my_bootom"
        android:layout_below="@+id/rl_my_main"
        android:background="@drawable/bg_yhbj"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/rl_my_adress"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="客户管理密码"
                android:layout_toRightOf="@+id/iv_my_mm"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:textColor="#6d6d6d"
                android:textSize="@dimen/sp_15"
                />
            <ImageView
                android:id="@+id/iv_my_mm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:src="@mipmap/my_pass"
                />
            <ImageView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_15"
                android:src="@mipmap/menu_xzn"
                />
            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_20"
                android:background="#eeeeee"
                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_my_xxadress"
            android:layout_below="@+id/rl_my_adress"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="使用指南"
                android:layout_toRightOf="@+id/iv_my_syzn"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:textColor="#6d6d6d"
                android:textSize="@dimen/sp_15"
                />
            <ImageView
                android:id="@+id/iv_my_syzn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:src="@mipmap/my_syzn"
                />
            <ImageView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_15"
                android:src="@mipmap/menu_xzn"
                />
            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_20"
                android:background="#eeeeee"
                />
        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rl_my_last"
        android:layout_below="@+id/rl_my_bootom"
        android:background="@drawable/bg_yhbj"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginRight="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/rl_my_sz"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_45">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="设置"
                android:layout_toRightOf="@+id/iv_my_sz"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:textColor="#6d6d6d"
                android:textSize="@dimen/sp_15"
                />
            <ImageView
                android:id="@+id/iv_my_sz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:src="@mipmap/my_sz"
                />
            <ImageView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/dp_15"
                android:src="@mipmap/menu_xzn"
                />
            <View
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_marginRight="@dimen/dp_20"
                android:background="#eeeeee"
                />
        </RelativeLayout>


    </RelativeLayout>


</RelativeLayout>
